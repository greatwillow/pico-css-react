{
	"verbose": true,
	"transform": {
		"^.+\\.(t|j)sx?$": "ts-jest",
		"^.+\\.css$": "jest-transform-css",
		"\\.(jpg|jpeg|png|gif|webp|svg)$": "jest-transform-file"
	},
	"testRegex": "(/test/.*\\.spec\\.tsx)$",
	"setupFilesAfterEnv": [
		"./test/configuration/setupTests.ts"
	],
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js",
		"jsx",
		"json",
		"node"
	]
}